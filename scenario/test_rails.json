{
    "_description": "Test RAILS on mnist image classification",
    "adhoc": null,
    "attack": {
        "knowledge": "white",
        "kwargs": {
            "batch_size": 1,
            "eps": 0.2,
            "eps_step": 0.1,
            "minimal": false,
            "num_random_init": 0,
            "targeted": false
        },
        "module": "art.attacks.evasion",
