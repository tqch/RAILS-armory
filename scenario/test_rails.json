        },
        "model_kwargs": {"kwargs_path":"aise_default_config.json"},
        "module": "rails",
        "name": "get_art_model",
        "weights_file": "model_weights/mnistmodel.pt",
        "wrapper_kwargs": {}
    },
    "scenario": {
        "kwargs": {},
        "module": "armory.scenarios.image_classification",
        "name": "ImageClassificationTask"
    },
    "sysconfig": {
        "docker_image": "twosixarmory/tf1:0.12.1",
        "external_github_repo": "tqch/aise_eval",
        "gpus": "all",
        "output_dir": null,
        "output_filename": null,
        "use_gpu": true
    }
}
