{
    "_description": "Evaluate RAILS on mnist image classification task by Wasserstein attacks",
    "adhoc": null,
    "attack": {
        "knowledge": "white",
        "kwargs": {
            "batch_size": 100,
            "eps": 0.031,
            "eps_step": 0.007,
            "max_iter": 10,
            "num_random_init": 1,
            "random_eps": false,
            "targeted": false,
            "verbose": false
        },
        "module": "art.attacks.evasion",
        "name": "ProjectedGradientDescent",
        "use_label": true
    },
    "dataset": {
        "batch_size": 100,
        "framework": "numpy",
        "module": "armory.data.datasets",
        "name": "cifar10"
    },
    "defense": null,
    "metric": {
        "means": true,
        "perturbation": "linf",
        "record_metric_per_sample": false,
        "task": [
            "categorical_accuracy"
        ]
    },
    "model": {
        "fit": false,
        "fit_kwargs": {
            "nb_epochs": 20
        },
        "model_kwargs": {},
        "module": "dknn_armory",
        "name": "get_art_model",
        "weights_file": "cifar_rails.pt",
        "wrapper_kwargs": {"channels_first": false}
    },
    "scenario": {
        "kwargs": {},
        "module": "armory.scenarios.image_classification",
        "name": "ImageClassificationTask"
    },
    "sysconfig": {
        "docker_image": "twosixarmory/pytorch:0.12.1",
        "external_github_repo": null,
        "gpus": "all",
        "output_dir": null,
        "output_filename": null,
        "use_gpu": false
    }
}
