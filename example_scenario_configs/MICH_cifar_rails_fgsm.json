{
    "_description": "Evaluate RAILS on cifar-10 image classification by FGSM attacks",
    "adhoc": null,
    "attack": {
        "knowledge": "white",
        "kwargs": {
            "batch_size": 1000,
            "eps": 0.031,
            "eps_step": 0.1,
            "minimal": false,
            "num_random_init": 0,
            "targeted": false
        },
        "module": "art.attacks.evasion",
        "name": "FastGradientMethod",
        "use_label": true
    },
    "dataset": {
        "batch_size": 1000,
        "framework": "numpy",
        "module": "armory.data.datasets",
        "name": "cifar10"
    },
    "defense": null,
    "metric": {
        "means": true,
        "perturbation": "linf",
        "record_metric_per_sample": false,
        "task": [
            "categorical_accuracy"
        ]
    },
    "model": {
        "fit": false,
        "fit_kwargs": {
            "nb_epochs": 20
        },
        "model_kwargs": {
            "aise_params": {
                "0": {
                    "sampling_temperature": 3,
                    "max_generation": 10
                },
                "1": {
                    "sampling_temperature": 18,
                    "max_generation": 10
                },
                "2": {
                    "sampling_temperature": 30,
                    "max_generation": 20
                },
                "3": {
                    "sampling_temperature": 300,
                    "max_generation": 20
                }
            },
            "hidden_layers": [
                2,
                3
				],
			"attack_cnn": true,
			"use_dknn": false
        },
        "module": "rails_cifar10",
        "name": "get_art_model",
        "weights_file": "MICH_cifar_rails.pt",
        "wrapper_kwargs": {"channels_first": false}
    },
    "scenario": {
        "kwargs": {},
        "module": "armory.scenarios.image_classification",
        "name": "ImageClassificationTask"
    },
    "sysconfig": {
        "docker_image": "twosixarmory/pytorch:0.12.1",
        "external_github_repo": "tqch/aise_eval",
        "gpus": "all",
        "output_dir": null,
        "output_filename": null,
        "use_gpu": false
    }
}
