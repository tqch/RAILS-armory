{
    "_description": "Evaluate RAILS on mnist image classification task by threshold attacks",
    "adhoc": null,
	"attack": {
        "knowledge": "black",
        "kwargs": {
            "th": 128,
            "es": 0
        },
        "module": "art.attacks.evasion",
        "name": "ThresholdAttack",
        "use_label": false
    },
    "dataset": {
        "batch_size": 1,
        "framework": "numpy",
        "module": "armory.data.datasets",
        "name": "mnist"
    },
    "defense": null,
    "metric": {
        "means": true,
        "perturbation": "linf",
        "record_metric_per_sample": false,
        "task": [
            "categorical_accuracy"
        ]
    },
    "model": {
        "fit": false,
        "fit_kwargs": {
            "nb_epochs": 20
        },
        "model_kwargs": {
            "aise_params": {
                "0": {
                    "sampling_temperature": 3
                },
                "1": {
                    "sampling_temperature": 18
                },
                "2": {
                    "sampling_temperature": 18
                },
                "3": {
                    "sampling_temperature": 72
                }
            },
            "hidden_layers": [
                0,
                1,
                2,
                3
				],
            "attack_cnn": false
        },
        "module": "rails",
        "name": "get_art_model",
        "weights_file": "mnist_rails_test.pt",
        "wrapper_kwargs": {"channels_first": false}
    },
    "scenario": {
        "kwargs": {},
        "module": "armory.scenarios.image_classification",
        "name": "ImageClassificationTask"
    },
    "sysconfig": {
        "docker_image": "twosixarmory/pytorch:0.12.1",
        "external_github_repo": "tqch/aise_eval",
        "gpus": "all",
        "output_dir": null,
        "output_filename": null,
        "use_gpu": false
    }
}